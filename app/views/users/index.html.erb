<h2>Users</h2>

<table class="table">
  <thead>
    <tr>
      <th>name</th>
      <th>introduction</th>
    </tr>
  </thead>
<tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= image_tag user.get_profile_image(100,100) %></td>
    <td><%= @users.name %></td>
    <td><%= link_to "Show", user_path(user.id) %></td>
  </tr>
<% end %>
</table>